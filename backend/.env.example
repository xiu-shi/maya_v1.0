# AI Builders MCP Server Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Required: Your AI Builders API token
AI_BUILDER_TOKEN=sk_your_token_here

# Server Configuration
NODE_ENV=development
PORT=3000

# CORS Configuration (comma-separated list of allowed origins)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,https://agents.janetxiushi.me,https://janetxiushi.me

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=20

# Request Size Limits
MAX_REQUEST_SIZE=1mb
MAX_MESSAGE_LENGTH=2000
MAX_HISTORY_LENGTH=50

# Security
ENABLE_HTTPS_REDIRECT=false
TRUST_PROXY=false
